import java.util.*;
class task7{
    public static void main(String[] args){
        int input1=3521;
        int input2=2452;
        int input3=1352;
        int min=9;
        int a=input1/1000;
        int b=(input2%1000)/100;
        while(input3>0){
            int digit=input3%10;
            if(digit<min){
                min=digit;
            }
            input3=input3/10;

        }
        System.out.println(a*b+min);
    }
}
