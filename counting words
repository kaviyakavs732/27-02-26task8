import java.util.*;
class string1{
    public static void main(String[] args){
    String a="  vel  tech high tech  eng college  ";
    int count=0;
    for(int i=0;i<a.length();i++){
    if(((a.charAt(i)>='a' && a.charAt(i)<='z') ||( a.charAt(i)>='A' && a.charAt(i)<='Z'))&&(i==0||a.charAt(i-1)==' ')){
    count++;}
    }
    System.out.println("count of words:  "+count);
}
}
